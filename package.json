{
  "name": "proxy-provider",
  "description": "Free proxy api.",
  "version": "1.0.4",
  "author": {
    "email": "simonegauli@gmail.com",
    "name": "Simone Gauli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pnlvfx/proxy-provider.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/provider.js",
  "module": "./dist/provider.js",
  "types": "./dist/provider.d.ts",
  "exports": {
    ".": {
      "types": "./dist/provider.d.ts",
      "import": "./dist/provider.js"
    },
    "./request": {
      "types": "./dist/request/agent.d.ts",
      "import": "./dist/request/agent.js"
    }
  },
  "scripts": {
    "tsc": "tsc --noEmit",
    "unused": "node tools/unused.ts",
    "dev": "tsx --no-cache --watch tools/cli.ts",
    "test": "",
    "lint": "eslint",
    "build": "rimraf dist && eslint && tsc -p tsconfig.build.json",
    "prepare": "husky",
    "prepack": "yarn build",
    "prepublishOnly": "yarn test && eslint",
    "update": "yarn upgrade-interactive",
    "publish": "node tools/publish.ts"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@goatjs/core": "^1.0.3",
    "@goatjs/dev": "^1.0.3",
    "@goatjs/git": "^1.0.3",
    "@goatjs/node": "^1.0.3",
    "@goatjs/node-eslint": "^1.0.6",
    "@goatjs/rimraf": "^1.0.3",
    "@goatjs/storage": "^1.0.3",
    "@goatjs/ts-unused-exports": "^1.0.3",
    "@goatjs/typescript-config": "^1.0.3",
    "@types/node": "^24.0.13",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "undici": "^7.11.0",
    "zod": "^4.0.5"
  },
  "peerDependencies": {
    "@goatjs/core": "*",
    "@goatjs/node": "*",
    "@goatjs/storage": "*",
    "undici": "*",
    "zod": "*"
  },
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c"
}
